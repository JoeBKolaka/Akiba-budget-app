# Create a new .gitignore
cat > .gitignore << 'EOF'
# Flutter
*.a
*.o
*.so
*.dylib
*.class
*.swp
*.swo
*~
.DS_Store
.atom/
.buildlog/
.history
.svn/
migrate_working_dir/

# IntelliJ/Android Studio
*.iml
.idea/
.idea/workspace.xml
.idea/tasks.xml
.idea/gradle.xml
.idea/assetWizardSettings.xml
.idea/dictionaries
.idea/libraries
*.iws

# Visual Studio Code
.vscode/*
!.vscode/settings.json
!.vscode/tasks.json
!.vscode/launch.json
!.vscode/extensions.json
.history/

# Flutter/Dart/Pub
**/doc/api/
.dart_tool/
.flutter-plugins
.flutter-plugins-dependencies
.packages
.pub-cache/
.pub/
/build/
/coverage/

# Android
**/android/**/gradle-wrapper.jar
**/android/.gradle
**/android/captures/
**/android/gradle/wrapper/gradle-wrapper.properties
**/android/local.properties
**/android/key.properties
**/android/**/GeneratedPluginRegistrant.java
**/android/app/debug/
**/android/app/profile/
**/android/app/release/

# iOS
**/ios/**/*.mode1v3
**/ios/**/*.mode2v3
**/ios/**/*.moved-aside
**/ios/**/*.pbxuser
**/ios/**/*.perspectivev3
**/ios/**/*sync/
**/ios/.symlinks/
**/ios/Flutter/App.framework
**/ios/Flutter/Flutter.framework
**/ios/Flutter/Flutter.podspec
**/ios/Flutter/Generated.xcconfig
**/ios/Flutter/ephemeral/
**/ios/Flutter/app.flx
**/ios/Flutter/app.zip
**/ios/Flutter/flutter_assets/
**/ios/Flutter/flutter_export_environment.sh
**/ios/ServiceDefinitions.json
**/ios/Runner/GeneratedPluginRegistrant.*

# Build
**/android/**/build
**/ios/**/build
*.apk
*.ipa
*.dmg

# Symbolication related
app.*.symbols

# Obfuscation related
app.*.map.json

# Exceptions to the above (if you want to track these)
!**/ios/Runner/Info.plist
!**/ios/Runner/Assets.xcassets/LaunchImage.imageset/launch_image.png
!**/ios/Runner/Assets.xcassets/LaunchImage.imageset/README.md
!**/ios/Runner/Assets.xcassets/AppIcon.appiconset/app_icon.png
!**/ios/Runner/Assets.xcassets/AppIcon.appiconset/README.md
